@model Car.Model.Vehicle
@{
    ViewData["Title"] = "GetBestValue";
}

<h1>Get Best Value Car</h1>
<h1></h1>
<h2>The Best Value is: @ViewBag.value</h2>

<table class="table table-striped table-dark">
    <tr>
        <td>Make</td>
        <td>Mode</td>
        <td>Color</td>
        <td>Year</td>
        <td>Price</td>
        <td>Tcc Rating</td>
        <td>Hwy MPG</td>
    </tr>

    <tr>
        <td>@Model.Make</td>
        <td>@Model.Model</td>
        <td>@Model.Color</td>
        <td>@Model.Year</td>
        <td>@Model.Price</td>
        <td>@Model.TCCRating</td>
        <td>@Model.HwyMPG</td>
    </tr>

</table>


