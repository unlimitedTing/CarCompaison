@model List<MVCCar.ViewModel.CarFuleConsumptionViewModel>
@{
    ViewData["Title"] = "GetFuleConsumption";
}

<h1>Get Fule Consumption</h1>
<h1></h1>
<h2>Input Distance:@ViewBag.Distance</h2>

<table class="table table-striped table-dark">
    <tr>
        <td>Make</td>
        <td>Mode</td>
        <td>Color</td>
        <td>Year</td>
        <td>Tcc Rating</td>
        <td>Hwy MPG</td>
        <td>Distance</td>
        <td>Fule Consumption</td>
    </tr>
    @foreach (var vehicle in Model) // declare the model, Model would be the body
    {
        <tr>
            <td>@vehicle.Make</td>
            <td>@vehicle.Mode</td>
            <td>@vehicle.Color</td>
            <td>@vehicle.Year</td>
            <td>@vehicle.TccRating</td>
            <td>@vehicle.MPG</td>
            <td>@ViewBag.Distance</td>
            <td>@vehicle.FuleConsumption</td>
            
        </tr>
    }
</table>


