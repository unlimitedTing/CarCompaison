@model  List<Car.Model.Vehicle>
@{
    ViewData["Title"] = "GetAllOrderByPrice";
}

<h1>Get All Order By Price</h1>

<table class="table table-striped table-dark">
    <tr>
        <td>Make</td>
        <td>Mode</td>
        <td>Color</td>
        <td>Year</td>
        <td>Price</td>
        <td>Tcc Rating</td>
        <td>Hwy MPG</td>
    </tr>
    @foreach (var vehicle in Model) // declare the model, Model would be the body
    {
        <tr>
            <td>@vehicle.Make</td>
            <td>@vehicle.Model</td>
            <td>@vehicle.Color</td>
            <td>@vehicle.Year</td>
            <td>@vehicle.Price</td>
            <td>@vehicle.TCCRating</td>
            <td>@vehicle.HwyMPG</td>
        </tr>
    }
</table>

